  \r\n\r\n</TD>  \r\n\r\n<TD width=\"23\" valign=\"top\" style='width:17.0pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 17  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"34\" valign=\"top\" style='width:25.5pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"208\" valign=\"top\" style='width:155.9pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Fólkaflokkurin (FF)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"19\" valign=\"top\" style='width:14.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR style='mso-yfti-lastrow:yes'>  \r\n\r\n<TD width=\"246\" valign=\"top\" style='width:184.25pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Socialistisk Folkeparti (SF)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"23\" valign=\"top\" style='width:17.0pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 11  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"34\" valign=\"top\" style='width:25.5pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"208\" valign=\"top\" style='width:155.9pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"19\" valign=\"top\" style='width:14.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n</TABLE>  \r\n\r\n<P class=\"TabelFod\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n<P class=\"Slutnotetekst\"><SPAN class=\"msoIns\">  \r\n\r\n<INS cite=\"mailto:FTCHRJ\" datetime=\"2005-07-11T11:12\">  \r\n\r\n &nbsp;  \r\n\r\n</INS>  \r\n\r\n</SPAN>  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoEndnoteText\" style='mso-prop-change:FTCHRJ 20050711T1112'>  \r\n\r\n<SPAN class=