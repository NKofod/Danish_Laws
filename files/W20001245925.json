    "documentHtml": "<div class=\"bjelke\">Den fulde tekst</div><DIV CLASS=PARA ID=\"INDHOLD\"> \r\n\r\n\r\n\r\n<P class=\"MsoNormal\" align=\"center\"  \r\n\r\n style='margin-left:13.0pt;text-align:center;line-height:14.35pt;mso-line  \r\n\r\n -height-rule:exactly;tab-stops:-29.55pt 12.95pt 55.45pt 97.95pt 140.45pt  \r\n\r\n 182.95pt 225.45pt 267.95pt 310.45pt 352.95pt 395.45pt 437.95pt 480.45pt  \r\n\r\n 522.95pt 565.45pt 607.95pt 650.45pt 692.95pt 735.45pt 777.95pt'>  \r\n\r\n<B>  \r\n\r\n K  \r\n\r\n E N D E L S E  \r\n\r\n</B>  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoNormal\" align=\"center\"  \r\n\r\n style='margin-left:13.0pt;text-align:center;line-height:14.35pt;mso-line  \r\n\r\n -height-rule:exactly;tab-stops:-29.55pt 12.95pt 55.45pt 97.95pt 140.45pt  \r\n\r\n 182.95pt 225.45pt 267.95pt 310.45pt 352.95pt 395.45pt 437.95pt 480.45pt  \r\n\r\n 522.95pt 565.45pt 607.95pt 650.45pt 692.95pt 735.45pt 777.95pt'>  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoNormal\" align=\"center\"  \r\n\r\n style='margin-left:13.0pt;text-align:center;line-height:14.35pt;mso-line  \r\n\r\n -height-rule:exactly;tab-stops:-29.55pt 12.95pt 55.45pt 97.95pt 140.45pt  \r\n\r\n 182.95pt 225.45pt 267.95pt 310.45pt 352.95pt 395.45pt 437.95pt 480.45pt  \r\n\r\n 522.95pt 565.45pt 607.95pt 650.45pt 692.95pt 735.45pt 777.95pt'>  \r\n\r\n afsagt  \r\n\r\n af Tvistighedsnævnet den 10. november 2000 i sag nr. 22.1999  \r\n\r\n</SPAN>  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoNormal\"  \r\n\r\n style='margin-left:13.0pt;text-align:justify;line-height:14.35pt;mso-line  \r\n\r\n -height-rule:exactly;tab-stops:-29.55pt 12.95pt 55.45pt 97.95pt 140.45pt  \r\n\r\n 182.95pt 225.45pt 267.95pt 310.45pt 352.95pt 395.45pt 437.95pt 480.45pt  \r\n\r\n 522.95pt 565.45pt 607.95pt 650.45pt 692.95pt 735.45pt 777.95pt'>  \r\n\r\n &nbsp;  \r\n\r\n</SPAN>  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoNormal\"  \r\n\r\n style='margin-left:13.0pt;line-height:14.35pt;mso-line-height-rule:exactly  \r\n\r\n ;tab-stops:-29.55pt 12.