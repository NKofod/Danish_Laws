spacing=\"0\" cellpadding=\"0\"  \r\n\r\n  style='border-collapse:collapse;mso-table-layout-alt:fixed;mso-padding-alt:  \r\n\r\n 0cm 0cm 0cm 0cm'>  \r\n\r\n<THEAD>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelOverskrift\">  \r\n\r\n Bilagsnr.  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelOverskrift\">  \r\n\r\n Titel  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n</THEAD>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Høringssvar samt høringsnotat, fra økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 2  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Udkast til tidsplan for udvalgets behandling af  \r\n\r\n lovforslagene  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 3  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Tidsplan for udvalgets behandling af lovforslagene  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 4  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 1 om et dansk aktieselskab, som driver import- og  \r\n\r\n eksportvirksomhed med flere andre lande m.v., til økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 2 om en mindre dansk bankvirksomheds  \r\n\r\n fusionsmuligheder med udenlandske banker m.v., til økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 3 om begrænsninger for, hvor hyppigt et SE-selskab  \r\n\r\n kan flytte mellem forskellige EU-lande, til økonomi- og erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 4 om, hvornår skatteligningen ophører for et  \r\n\r\n selskab, der er under flytning til et andet EU-land, til økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 5 om fusionsbeskatning i forbindelse med fusion og  \r\n\r\n domicilflytning m.v., til økonomi- og erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 6 om kontrol ved flytning af et SE-selskab, til  \r\n\r\n økonomi- og erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 7 om, i hvilke navngivne EU-lande skatteligningen  \r\n\r\n anses for at være væsentligt mindre effektiv end i Danmark, til økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Spm. 8 om oversendelse af senest opdateret liste over  \r\n\r\n selskabsskatteprocenterne i nuværende og kommende EU-lande, til økonomi- og  \r\n\r\n erhvervsministeren  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"76\" valign=\"top\" style='width:2.0cm;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 5  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"454\" valign=\"top\" style='width:340.15pt;padding:0cm 0cm 0cm 0cm'>  \r\