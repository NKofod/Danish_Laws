='width:184.25pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Det Konservative Folkeparti (KF)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"23\" valign=\"top\" style='width:17.0pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 18  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"34\" valign=\"top\" style='width:25.5pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"208\" valign=\"top\" style='width:155.9pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Siumut (SIU)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"19\" valign=\"top\" style='width:14.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR>  \r\n\r\n<TD width=\"246\" valign=\"top\" style='width:184.25pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Det Radikale Venstre (RV)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"23\" valign=\"top\" style='width:17.0pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 17  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"34\" valign=\"top\" style='width:25.5pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"208\" valign=\"top\" style='width:155.9pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Fólkaflokkurin (FF)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"19\" valign=\"top\" style='width:14.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR style='mso-yfti-lastrow:yes'>  \r\n\r\n<TD width=\"246\" valign=\"top\" style='width:184.25pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Socialistisk Folkeparti (SF)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"23\" valign=\"top\" style='width:17.0pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 11  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"34\" valign=\"top\" style='width:25.5pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"208\" valign=\"top\" style='width:155.9pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"19\" valign=\"top\" style='width:14.15pt;padding:0cm 0cm 0cm 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n</TABLE>  \r\n\r\n<P class=\"TabelFod\">  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n<P class=\"MsoEndnoteText\" style='mso-prop-change:FTNAKL 20050609T1446'>  \r\n\r\n &nbsp;  \r\n\r\n</P>  \r\n\r\n<A NAME=\"\"B1\"\"></A>\r\n\r\n<DIV CLASS=BILAG ID=\"B1\">  \r\n\r\n<BR>  \r\n\r\n<HR width=70% align=center>  \r\n\r\n<P class=\"Bilagstitel\">Bilag 1 </P>  \r\n\r\n<P class=\"BilagsOverskrift\"> Oversigt over bilag vedrørende B 79 </P>  \r\n\r\n<TABLE class=\"MsoNormalTable\" border=\"0\" cellpadding=\"0\"  \r\n\r\n style='mso-cellspacing:1.5pt;mso-padding-left-alt:0cm;mso-padding-right-alt  \r\n\r\n :0cm'>  \r\n\r\n<THEAD>  \r\n\r\n<TR style='mso-yfti-irow:0'>  \r\n\r\n<TD width=\"73\" style='width:54.45pt;padding:.75pt 0cm .75pt 0cm'>  \r\n\r\n<P class=\"TabelOverskrift\">  \r\n\r\n Bilagsnr.  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"451\" style='width:337.9pt;padding:.75pt 0cm .75pt 0cm'>  \r\n\r\n<P class=\"TabelOverskrift\">  \r\n\r\n Titel  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n</THEAD>  \r\n\r\n<TR style='mso-yfti-irow:1'>  \r\n\r\n<TD width=\"73\" valign=\"top\" style='width:54.45pt;padding:.75pt 0cm .75pt  \r\n\r\n 0cm'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"451\" style='width:337.9pt;padding:.75pt 0cm .75pt 0cm'>  \r\n\r\n<P class=\"TabelTekst\">