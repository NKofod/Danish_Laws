s=\"TabelTekst\">  \r\n\r\n Drænet gulv + spalter (50/50)  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top\" style='width:132.3pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Gylle  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top\" style='width:132.3pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 5,6  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR style='height:2.0pt'>  \r\n\r\n<TD width=\"176\" valign=\"top\" style='width:132.25pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Fast gulv  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top\" style='width:132.3pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Fast gødning  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n + ajle  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top\" style='width:132.3pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 1,6  \r\n\r\n</P>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n 2,4  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n</TR>  \r\n\r\n<TR style='mso-yfti-lastrow:yes;height:2.0pt'>  \r\n\r\n<TD width=\"176\" valign=\"top\"  \r\n\r\n style='width:132.25pt;border:none;border-bottom:solid windowtext  \r\n\r\n 1.0pt;mso-border-bottom-alt:solid windowtext .5pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Dybstrøelse  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top\"  \r\n\r\n style='width:132.3pt;border:none;border-bottom:solid windowtext  \r\n\r\n 1.0pt;mso-border-bottom-alt:solid windowtext .5pt;padding:0cm 7.05pt 0cm  \r\n\r\n 7.05pt;height:2.0pt'>  \r\n\r\n<P class=\"TabelTekst\">  \r\n\r\n Dybstrøelse  \r\n\r\n</P>  \r\n\r\n</TD>  \r\n\r\n<TD width=\"176\" valign=\"top