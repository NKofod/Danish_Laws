    "documentHtml": "<div class=\"bjelke\">Den fulde tekst</div><DIV CLASS=PARA ID=\"INDHOLD\"> \r\n\r\n\r\n\r\n<FONT SIZE=5><P ALIGN=\"CENTER\">NOA-Meddelelse om som f&oslash;lge af  \r\n\r\n &aelig;ndringerne i lov om en aktiv arbejdsmarkedspolitik og  \r\n\r\n bekendtg&oslash;relse om en aktiv arbejdsmarkedsindsats</P>  \r\n\r\n</FONT>  \r\n\r\n<P>&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">F&oslash;lgende NOA-Meddelelser oph&aelig;ves:</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\"></P>  \r\n\r\n<TABLE BORDER CELLSPACING=1 CELLPADDING=4 WIDTH=651>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<B><P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Meddelelse nr.:</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\"></B></TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<B><P ALIGN=\"JUSTIFY\">Vedr&oslash;rende:</B></TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<B><P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Begrundelse for</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">oph&aelig;velsen:</B></TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 6/94</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 21. marts 1994</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Tvungen ferie i relation</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">til beregning af</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">minimumsretten p&aring; 12</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">m&aring;neder</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Retten til et rimeligt</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">forl&oslash;b (minimumsretten) </P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">blev oph&aelig;vet pr. 1. januar 1998</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 9/94</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 23. marts 1994</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Etableringsydelse/iv&aelig;rk-</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">s&aelig;tterydelse</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Tilbud om etablerings-</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">ydelse blev oph&aelig;vet pr.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">1. januar 1998</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 10/94</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 15. april 1994</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Deltidsforsikredes ret til</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">fuldtids jobtr&aelig;ning</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Der er ikke l&aelig;ngere ret</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">til selvfunden jobtr&aelig;ning</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\"></TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 16/94</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 7. juli 1994</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Opg&oslash;relse af</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">minimumsretten i</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">relation til jobtr&aelig;ning</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Retten til et rimeligt</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">forl&oslash;b (minimumsretten)</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">blev oph&aelig;vet pr. 1.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">januar 1998</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 7/96</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 29. august 1996</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Oprettelse af puljejob, jf.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">§ 37 i aktivloven</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Der administreres ikke</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">l&aelig;ngere med en</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">forh&aring;ndsfordelt kvote til</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">stat, amter og kommuner</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 10/96</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 6. november 1996</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Forl&aelig;ngelse af en</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">periode med uddannel-</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">sesgodtg&oslash;relse</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">F&oslash;lger nu af § 110, stk.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">6, i aktivbekendtg&oslash;relsen</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">A. 2/97</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">af 25. februar 1997</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">St&oslash;tte til besk&aelig;ftigede,</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">der deltager i uddannelse</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">som led i jobrotation  </P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">aktivbekendtg&oslash;relsens §</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">88 a, stk. 2</TD>  \r\n\r\n<TD WIDTH=\"33%\" VALIGN=\"TOP\">  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">F&oslash;lger nu af § 102, stk.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">4, og § 103, stk. 3, i</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">aktivbekendtg&oslash;relsen</TD>  \r\n\r\n</TR>  \r\n\r\n</TABLE>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Ved &aelig;ndringen af bekendtg&oslash;relse om en aktiv  \r\n\r\n arbejdsmarkedsindsats pr. 1. januar 1999 blev paragrafnummereringen  \r\n\r\n &aelig;ndret.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">Af hensyn til forst&aring;elsen af NOA-Meddelelserne fra  \r\n\r\n f&oslash;r denne dato vedl&aelig;gges som bilag en paragrafn&oslash;gle, der  \r\n\r\n viser de tidligere og nuv&aelig;rende paragrafnumre.</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">&nbsp;</P>  \r\n\r\n<P ALIGN=\"JUSTIFY\">NOA-Meddelelse nr. A. 1/99 af 23. april 1999 oph&aelig;ves  \r\n\r\n herved.</P>  \r\n\r\n<A NAME=\"\"B1\"\"></A>\r\n\r\n<DIV CLASS=BILAG ID=\"B1\">  \r\n\r\n<HR width=70% align=center>  \r\n\r\n<B><FONT SIZE=4><P ALIGN=\"RIGHT\">Bilag 1</P>  \r\n\r\n</B>  \r\n\r\n</FONT>  \r\n\r\n<B><P>&AElig;ndrede paragrafnumre i aktivbekendtg&oslash;relsen pr. 1. januar  \r\n\r\n 1999.</P>  \r\n\r\n</B>  \r\n\r\n<TABLE BORDER CELLSPACING=1 CELLPADDING=4 WIDTH=651>  \r\n\r\n<TR><TD WIDTH=\"50%\" VALIGN=\"TOP\">  \r\n\r\n<P>Tidligere §-nr.</TD>  \r\n\r\n<TD WIDTH=\"50%\" VALIGN=\"TOP\">  \r\n\r\n<P>Nuv&aelig;rende §-nr.</TD>  \r\n\r\n</TR>  \r\n\r\n<TR><TD WIDTH=\"50%\" VALIGN=\"TOP\">  \r\n\r\n<P>§ 1</P>  \r\n\r\n<P>§ 2</P>  \r\n\r\n<P>§ 3</P>  \r\n\r\n<P>§ 4</P>  \r\n\r\n<P>§ 5</P>  \r\n\r\n<P>-</P>  \r\n\r\n<P>§ 6</P>  \r\n\r\n<P>§ 7</P>  \r\n\r\n<P>§ 8</P>  \r\n\r\n<P>§ 9</P>  \r\n\r\n<P>§ 10</P>  \r\n\r\n<P>§ 11</P>  \r\n\r\n<P>§ 12</P>  \r\n\r\n<P>§ 13</P>  \r\n\r\n<P>§ 13 a</P>  \r\n\r\n<P>§ 14</P>  \r\n\r\n<P>§ 14 a</P>  \r\n\r\n<P>§ 15</P>  \r\n\r\n<P>§ 16</P>  \r\n\r\n<P>§ 17</P>  \r\n\r\n<P>§ 18</P>  \r\n\r\n<P>-</P